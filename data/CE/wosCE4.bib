@article{ ISI:000281492200003,
Author = {Geng, Yong and Wang Xinbei and Zhu Qinghua and Zhao Hengxin},
Title = {{Regional initiatives on promoting cleaner production in China: a case of
   Liaoning}},
Journal = {{JOURNAL OF CLEANER PRODUCTION}},
Year = {{2010}},

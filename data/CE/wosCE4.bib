@article{ ISI:000291905000007,
Author = {Zhang, Tongzhu and Chu, Jiangwei and Wang, Xueping and Liu, Xianghai and
   Cui, Pengfei},
Title = {{Development pattern and enhancing system of automotive components
   remanufacturing industry in China}},
Journal = {{RESOURCES CONSERVATION AND RECYCLING}},

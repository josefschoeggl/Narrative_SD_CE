@article{ ISI:000463655200016,
Author = {Gusmerotti, Natalia Marzia and Corsini, Filippo and Testa, Francesco and
   Rizzi, Francesco and Frey, Marco},
Title = {{OUTSOURCING ENVIRONMENTAL SERVICES: NEW INSIGHTS INTO THE FACTORS
   INFLUENCING THE CIRCULAR ECONOMY}},
Journal = {{ENVIRONMENTAL ENGINEERING AND MANAGEMENT JOURNAL}},
Year = {{2019}},
Volume = {{18}},
Number = {{2}},
Pages = {{433-446}},
Month = {{FEB}},
Abstract = {{Recent studies have demonstrated how buyers can influence the
   environmental performance of suppliers. However, although industrial
   waste accounts for much of the waste generated in the west, little is
   known about how industrial waste is managed downstream. We present an
   analysis of the drivers influencing the decision to opt for
   waste-to-recovery rather than disposal facilities. The drivers were
   investigated by a questionnaire survey. Findings show that the
   manufacturing industry is more likely to send waste to recovery rather
   than disposal facilities, as a result of factors such as the service
   provider's reputation, the presence of a broker in the transaction, and
   pressure from public authorities.}},

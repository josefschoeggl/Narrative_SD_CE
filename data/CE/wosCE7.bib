@article{ ISI:000457150200048,
Author = {Marzorati, Stefania and Verotta, Luisella and Trasatti, Stefano P.},
Title = {{Green Corrosion Inhibitors from Natural Sources and Biomass Wastes}},
Journal = {{MOLECULES}},

@article{ ISI:000460498500001,
Author = {Alvarez-Cao, Maria-Efigenia and Cerdan, Maria-Esperanza and
   Gonzalez-Siso, Maria-Isabel and Becerra, Manuel},
Title = {{Bioconversion of Beet Molasses to Alpha-Galactosidase and Ethanol}},

@article{ ISI:000459976200081,
Author = {Kazmierczak, Urszula and Blachowski, Jan and Gorniak-Zimroz, Justyna},
Title = {{Multi-Criteria Analysis of Potential Applications of Waste from Rock
   Minerals Mining}},

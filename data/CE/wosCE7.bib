@article{ ISI:000457952400039,
Author = {Mendoza, Joan Manuel F. and D'Aponte, Francesco and Gualtieri, Diego and
   Azapagic, Adisa},
Title = {{Disposable baby diapers: Life cycle costs, eco-efficiency and circular
   economy}},
Journal = {{JOURNAL OF CLEANER PRODUCTION}},

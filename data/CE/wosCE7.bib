@article{ ISI:000506619600007,
Author = {Tacchini, Massimo and Burlini, Ilaria and Bernardi, Tatiana and De Risi,
   Carmela and Massi, Alessandro and Guerrini, Alessandra and Sacchetti,
   Gianni},
Title = {{Chemical characterisation, antioxidant and antimicrobial screening for
   the revaluation of wine supply chain by-products oriented to circular
   economy}},
Journal = {{PLANT BIOSYSTEMS}},
Year = {{2019}},
Volume = {{153}},
Number = {{6}},
Pages = {{809-816}},
Abstract = {{Aim of the project was the bioassay guided optimisation of extraction
   methods applied to wine chain by-product to obtain extracts, fractions
   and biologically active biomolecules with a possible use in the
   nutraceutical and cosmeceutical industry. Exhausted red and white grape
   marc were extracted using water:ethanol 50:50 with ultrasound assisted
   extraction and Naviglio (R) technology; and also with supercritical
   fluid extraction (SFE) and steam-distillation obtaining different
   phytocomplexes. Each extract was characterised by different molecular
   category: exhausted red grape marc (VCR) by anthocyanins, exhausted
   white grape marc (VCB) by flavonoids, and grapeseed (VIN) by
   proanthocyanins. SFE and steam-distillation, instead, highlighted the

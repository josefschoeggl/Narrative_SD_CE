@article{ ISI:000459027600021,
Author = {Hees, Timo and Zhong, Fan and Stuerzel, Markus and Muelhaupt, Rolf},
Title = {{Tailoring Hydrocarbon Polymers and All-Hydrocarbon Composites for
   Circular Economy}},
Journal = {{MACROMOLECULAR RAPID COMMUNICATIONS}},

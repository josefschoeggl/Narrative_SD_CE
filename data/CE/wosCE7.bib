@article{ ISI:000485195400002,
Author = {Feng Yanhong and Wang Zhongfu},
Title = {{Improved data envelopment analysis model based on geometric mean model}},
Journal = {{INTERNATIONAL JOURNAL OF COMPUTER APPLICATIONS IN TECHNOLOGY}},
Year = {{2019}},
Volume = {{61}},
Number = {{1-2, SI}},
Pages = {{8-15}},
Abstract = {{According to the characteristics of the circular economy conception of
   the under-development regions under new economy situation, the improved
   data envelopment analysis model based on geometric mean model is
   reconstructed in the paper through increasing unexpected input (waste
   recycling quantity) in the input and increasing unexpected output (waste
   discharge quantity) in the output; then, the improved data envelopment
   analysis model is adopted to evaluate the efficiency of the circular
   economy of the under-development regions in 31 provinces and cities in
   China during 2002-2012. The research result shows that the efficiency of
   the circular economy of the under-development regions in China is
   generally improved, and the scale efficiency gradually tends to reach
   the optimum efficiency, but the obvious insufficiency of pure technical
   efficiency has influenced the development level of the circular economy
   of the under-development regions in China.}},

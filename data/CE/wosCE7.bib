@article{ ISI:000455388900011,
Author = {Goyal, Ankur and Agrawal, Rajat and Chokhani, Rakesh Kumar and Saha,
   Chiraranjan},
Title = {{Waste reduction through Kaizen approach: A case study of a company in
   India}},
Journal = {{WASTE MANAGEMENT \& RESEARCH}},
Year = {{2019}},
Volume = {{37}},
Number = {{1}},
Pages = {{102-107}},
Month = {{JAN}},
Abstract = {{The growth in manufacturing activities for economic development is
   putting a lot of pressure on the environment. One such impact is due to
   generation of waste from hazardous material. The best way for hazardous
   waste management is either elimination or substitution with
   non-hazardous material. The case study in the present paper is focused
   on the problem of hazardous waste reduction in absence of non-hazardous
   substitution. In absence of any non-hazardous substitute of hazardous
   material, waste generation can be reduced using improvement techniques
   such as Kaizen to improve the manufacturing process. An Indian company
   improved the manufacturing procedure with very low cost to reduce waste
   generation of hazardous material. The outcome of this Kaizen project is
   the reduction of waste by 13.8\% at very low cost. Along with waste
   reduction, cost saving, and other resource savings are another benefits.
   This case study provides insight about the use of Kaizen for
   environmentally sustainable manufacturing.}},

@article{ ISI:000465613000118,
Author = {Nosratabadi, Saeed and Mosavi, Amir and Shamshirband, Shahaboddin and
   Zavadskas, Edmundas Kazimieras and Rakotonirainy, Andry and Chau, Kwok
   Wing},
Title = {{Sustainable Business Models: A Review}},

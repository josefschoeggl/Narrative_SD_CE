@article{ ISI:000464343400001,
Author = {Nerini, Francesco Fuso and Slob, Adriaan and Engstrom, Rebecka
   Ericsdotter and Trutnevyte, Evelina},
Title = {{A Research and Innovation Agenda for Zero-Emission European Cities}},
Journal = {{SUSTAINABILITY}},
Year = {{2019}},
Volume = {{11}},
Number = {{6}},
Month = {{MAR 20}},
Abstract = {{The Paris Agreement and SDG13 on Climate Action require a global drop in
   Green House Gases (GHG) emissions to stay within a ``well below 2
   degrees{''} climate change trajectory. Cities will play a key role in
   achieving this, being responsible for 60 to 80\% of the global GHG
   emissions depending on the estimate. This paper describes how Research
   and Innovation (R\&I) can play a key role in decarbonizing European
   cities, and the role that research and education institutions can play
   in that regard. The paper highlights critical R\&I actions in cities
   based on three pillars: (1) innovative technology and integration, (2)
   governance innovation, and (3) social innovation. Further, the research
   needed to harmonize climate mitigation and adaptation in cities are
   investigated.}},
Publisher = {{MDPI}},
Address = {{ST ALBAN-ANLAGE 66, CH-4052 BASEL, SWITZERLAND}},

@article{ ISI:000445733300015,
Author = {Lopes de Sousa Jabbour, Ana Beatriz and Chiappetta Jabbour, Charbel Jose
   and Godinho Filho, Moacir and Roubaud, David},
Title = {{Industry 4.0 and the circular economy: a proposed research agenda and
   original roadmap for sustainable operations}},
Journal = {{ANNALS OF OPERATIONS RESEARCH}},

@article{ ISI:000428163500006,
Author = {Hou, Hongying and Yu, Chengyi and Liu, Xianxi and Yao, Yuan and Liao,
   Qishu and Dai, Zhipeng and Li, Dongdong},
Title = {{Waste-loofah-derived carbon micro/nanoparticles for lithium ion battery
   anode}},
Journal = {{SURFACE INNOVATIONS}},

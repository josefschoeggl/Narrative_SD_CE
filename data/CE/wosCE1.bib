@article{ ISI:000434902400004,
Author = {Gaustad, Gabrielle and Krystofik, Mark and Bustamante, Michele and
   Badami, Kedar},
Title = {{Circular economy strategies for mitigating critical material supply
   issues}},
Journal = {{RESOURCES CONSERVATION AND RECYCLING}},

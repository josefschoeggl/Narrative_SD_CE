@article{ ISI:000432234700014,
Author = {Bolzonella, D. and Fatone, F. and Gottardo, M. and Frison, N.},
Title = {{Nutrients recovery from anaerobic digestate of agro-waste:
   Techno-economic assessment of full scale applications}},
Journal = {{JOURNAL OF ENVIRONMENTAL MANAGEMENT}},

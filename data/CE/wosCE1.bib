@article{ ISI:000449228700021,
Author = {Ayre, David},
Title = {{Technology advancing polymers and polymer composites towards
   sustainability: A review}},
Journal = {{CURRENT OPINION IN GREEN AND SUSTAINABLE CHEMISTRY}},
Year = {{2018}},
Volume = {{13}},
Number = {{SI}},
Pages = {{108-112}},
Month = {{OCT}},
Abstract = {{It is reported that plastic waste is accumulating in the oceans at an
   alarming rate. A significant proportion of this waste is plastic
   packaging materials, even though recycling routes and options are
   available to retain and reuse this oil-based resource. In addition to
   thermoplastic materials, fibre reinforced polymer materials are finding
   increased use and soon will become a concern with respect to disposal.
   This review presents current research and technology that aims to
   embrace plastics and polymer composites into a circular economy. The
   benefits and challenges associated with polymer recycling are
   highlighted, not least being the strategies required to encourage
   industry and society to recycle resources rather than dispose and renew.}},

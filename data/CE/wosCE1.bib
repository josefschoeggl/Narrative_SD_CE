@article{ ISI:000453889900016,
Author = {Novelli, Veronica and Geatti, Paola and Ceccon, Luciano and Bettarel,
   Alberto},
Title = {{ECO-INNOVATION IN VALCUCINE FOR A CIRCULAR ECONOMY}},

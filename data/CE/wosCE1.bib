@article{ ISI:000452344900081,
Author = {Muralidhara, Anitha and Tosi, Pierluigi and Mija, Alice and
   Sbirrazzuoli, Nicolas and Len, Christophe and Engelen, Victor and de
   Jong, Ed and Marlair, Guy},
Title = {{Insights on Thermal and Fire Hazards of Humins in Support of Their
   Sustainable Use in Advanced Biorefineries}},
Journal = {{ACS SUSTAINABLE CHEMISTRY \& ENGINEERING}},

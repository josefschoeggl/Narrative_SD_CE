Author = {Andretta, Alfonso and D'Addato, Filippo and Serrano-Bernardo, Francisco
   and Zamorano, Montserrat and Bonoli, Alessandra},
Title = {{ENVIRONMENTAL TAXES TO PROMOTE THE EU CIRCULAR ECONOMY'S STRATEGY: SPAIN
   vs. ITALY}},

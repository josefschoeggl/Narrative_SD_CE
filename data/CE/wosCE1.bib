@article{ ISI:000452024600072,
Author = {Oncioiu, Ionica and Grecu, Eugenia and Masu, Smaranda and Morariu,
   Florica and Popa, Maria},
Title = {{The effect of fly ash on sunflower growth and human health}},
Journal = {{ENVIRONMENTAL SCIENCE AND POLLUTION RESEARCH}},
Year = {{2018}},

@article{ ISI:000439467300016,
Author = {Qin, Zuodong and Sun, Mingxing and Luo, Xiaofang and Zhang, Hongrui and
   Xie, Jiyong and Chen, Haifeng and Yang, Lirong and Shi, Lei},
Title = {{Life-cycle assessment of tobacco stalk utilization}},

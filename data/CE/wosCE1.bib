@article{ ISI:000444358400088,
Author = {Blomsma, Fenna},
Title = {{Collective `action recipes' in a circular economy - On waste and
   resource management frameworks and their role in collective change}},
Journal = {{JOURNAL OF CLEANER PRODUCTION}},

@article{ ISI:000445575600014,
Author = {Vegera, Svetlana and Malei, Alena and Sapeha, Ina and Sushko, Volha},
Title = {{INFORMATION SUPPORT OF THE CIRCULAR ECONOMY: THE OBJECTS OF ACCOUNTING
   AT RECYCLING TECHNOLOGICAL CYCLE STAGES OF INDUSTRIAL WASTE}},

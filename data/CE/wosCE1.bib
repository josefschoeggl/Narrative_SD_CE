@article{ ISI:000440853700002,
Author = {Esposito, Mark and Tse, Terence and Soufani, Khaled},
Title = {{The circular economy: An opportunity for renewal, growth, and stability}},
Journal = {{THUNDERBIRD INTERNATIONAL BUSINESS REVIEW}},
Year = {{2018}},
Volume = {{60}},
Number = {{5, SI}},
Pages = {{725-728}},
Month = {{SEP-OCT}},
Abstract = {{Though governments and research institutions are giving more
   consideration than ever to the regenerative prospects of the circular
   economy, efficient solutions are still in need. A perfunctory look at
   the numbers shows some unsustainable and troubling math. It has been
   estimated that growing global consumer demands will require the natural
   resources of two Earths by the year 2030. Creating a circular economy
   that can meet those demands in the face of resource scarcity requires
   more than creating products from recycled plastic or building
   sustainable homes and offices.}},

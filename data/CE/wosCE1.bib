@article{ ISI:000452489500026,
Author = {Lopez-Uceda, Antonio and Galvin, Adela P. and Ayuso, Jesus and Ramon
   Jimenez, Jose and Vanwalleghem, Tom and Pena, Adolfo},
Title = {{Risk assessment by percolation leaching tests of extensive green roofs
   with fine fraction of mixed recycled aggregates from construction and
   demolition waste}},

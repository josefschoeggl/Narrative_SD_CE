@article{ ISI:000453820700003,
Author = {Tsolakis, Naoum and Srai, Jagjit Singh},
Title = {{Mapping supply dynamics in renewable feedstock enabled industries: A
   systems theory perspective on ``green' pharmaceuticals}},
Journal = {{OPERATIONS MANAGEMENT RESEARCH}},
Year = {{2018}},

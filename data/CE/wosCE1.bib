@article{ ISI:000440947600032,
Author = {Tantau, Adrian Dumitru and Maassen, Maria Alexandra and Fratila,
   Laurentiu},
Title = {{Models for Analyzing the Dependencies between Indicators for a Circular
   Economy in the European Union}},

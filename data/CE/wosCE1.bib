@article{ ISI:000432234700033,
Author = {Masi, E. and Rizzo, A. and Regelsberger, M.},
Title = {{The role of constructed wetlands in a new circular economy, resource
   oriented, and ecosystem services paradigm}},
Journal = {{JOURNAL OF ENVIRONMENTAL MANAGEMENT}},

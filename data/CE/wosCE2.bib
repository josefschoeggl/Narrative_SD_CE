@article{ ISI:000423002500050,
Author = {Zorpas, Antonis A. and Lasaridi, Katia and Pociovalisteanu, Diana
   Michaela and Loizia, Pantelitsa},
Title = {{Monitoring and evaluation of prevention activities regarding household
   organics waste from insular communities}},

@article{ ISI:000415942100015,
Author = {Bahn-Walkowiak, Bettina and Wilts, Henning},
Title = {{The institutional dimension of resource efficiency in a multi-level
   governance system-Implications for policy mix design}},
Journal = {{ENERGY RESEARCH \& SOCIAL SCIENCE}},

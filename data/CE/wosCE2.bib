@article{ ISI:000429385700008,
Author = {Ohno, Hajime and Sato, Hirokazu and Fukushima, Yasuhiro},
Title = {{Configuration of Materially Retained Carbon in Our Society: A
   WIO-MFA-Based Approach for Japan}},
Journal = {{ENVIRONMENTAL SCIENCE \& TECHNOLOGY}},

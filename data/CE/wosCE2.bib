@article{ ISI:000418658300001,
Author = {Karayannis, V. and Moutsatsou, A. and Domopoulou, A. and Katsika, E. and
   Drossou, C. and Baklavaridis, A.},
Title = {{Fired ceramics 100\% from lignite fly ash and waste glass cullet
   mixtures}},
Journal = {{JOURNAL OF BUILDING ENGINEERING}},
Year = {{2017}},
Volume = {{14}},
Pages = {{1-6}},
Month = {{NOV}},
Abstract = {{In the present study, the development of new building ceramics is
   investigated, using 100\% lignite fly ash (FA) and waste glass cullet
   (WGC) mixtures as secondary industrial raw materials towards circular
   economy. Thus, compacted and sintered (at 700 and 900 degrees C) ceramic
   bodies based on binary WGC/FA mixtures, with WGC loadings up to 15\%,
   were fabricated. The utilization of WGC (amorphous) aimed at lowering
   the sintering temperature of the mixture, for energy reduction purposes,
   via a better heat flux regulation in the material. The successful
   consolidation/densification of the ceramic microstructures, mainly
   composed of different silica phases, was achieved upon synergistic
   sintering at 900 degrees C for 2 h. Moreover, the successful
   consolidation/densification was confirmed by the SEM micrograph
   observation and the porosity evaluation from the SEM micrographs. The
   addition of WGC yielded to a drastic decrease in the porosity values
   (down to 12\%) for the samples sintered at 900 degrees C for 2 h. This
   porosity decrease favored, in turn, the substantial microhardness
   increase (up to 3833 HV) due to the pore sealing by the glassy phase of
   WGC. Moreover, an exponential relationship between microhardness and
   porosity was revealed. Finally, further investigation of the processing
   conditions is currently underway towards the optimization of the
   attained ceramic microstructures in order to meet the requirements of
   specific applications.}},

@article{ ISI:000444095300005,
Author = {Elomaa, H. and Halli, P. and Sirvioe, T. and Yliniemi, K. and Lundstrom,
   M.},
Title = {{A future application of pulse plating - silver recovery from
   hydrometallurgical bottom ash leachant}},
Journal = {{TRANSACTIONS OF THE INSTITUTE OF METAL FINISHING}},
Year = {{2018}},
Volume = {{96}},
Number = {{5}},
Pages = {{253-257}},
Abstract = {{In the current study, electrodeposition-redox replacement was applied to
   a hydrometallurgical solution with the main elements of Ca (13.8 g L-1),
   Al (4.7 g L-1), Cu (2.5 g L-1), Zn (1.2 g L-1), Fe (1.2 g L-1), S (1 g
   L-1), Mg (0.8 g L-1), P (0.5 g L-1) and Ag (3.5 ppm). The solution
   originated from the leaching experiment of incinerator plant bottom ash,
   which was dissolved into 2 M HCl media at T=30 degrees C. The resulting
   deposit on the electrode surface was analysed with SEM-EDS and the
   observed Ag/(Cu + Zn) ratio (0.3) indicated remarkable enrichment of
   silver on the surface, when compared to the ratio of these elements
   (Ag/(Cu+Zn)) in the solution (6.8 x 10(-5)). The enrichment of Ag vs.
   (Cu + Zn) could be demonstrated to increase ca. 4500 fold compared to
   the ratio of the elements in solution.}},

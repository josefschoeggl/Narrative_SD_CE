@article{ ISI:000426899200009,
Author = {Paul, Subhash and Dutta, Animesh and Defersha, Fantahun and Dubey,
   Brajesh},
Title = {{Municipal Food Waste to Biomethane and Biofertilizer: A Circular Economy
   Concept}},
Journal = {{WASTE AND BIOMASS VALORIZATION}},
Year = {{2018}},
Volume = {{9}},
Number = {{4}},
Pages = {{601-611}},
Month = {{APR}},
Abstract = {{This research is a dual resources recovery approach of biomethane and
   biofertilizer from as received municipal food waste (MFW) using the
   concept of circular economy. In this research MFW is first pretreated in
   hydromechanical process to produce broth suitable for anaerobic
   digestion (AD). The MFW broth then underwent mesophilic wet AD process
   to produce biomethane and biofertilizer. Three selected feedstock and
   inoculum ratios (F/I) of 1:2, 1:1 and 2:1 by volume for 30 days
   retention time were considered. Among three F/I ratios, the F/I ratio of
   1:1 digester was found to be the best batch condition for 30 days
   retention time for maximum biomethane production. MFW broth had ash
   content of 31.51\% of total solid (TS), higher heating value (HHV) of
   18.32 MJ kg(-1) of TS and C/N ratio of 15.29. The batch F/I of 1:1 AD
   produced the maximum biomethane of 347.69 L kg(-1) TS, 511.92 L kg(-1)

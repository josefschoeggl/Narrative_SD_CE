Author = {Morone, Milena and Costa, Giulia and Georgakopoulos, Evangelos and
   Manovic, Vasilije and Stendardo, Stefano and Baciocchi, Renato},
Title = {{Granulation-Carbonation Treatment of Alkali Activated Steel Slag for
   Secondary Aggregates Production}},
Journal = {{WASTE AND BIOMASS VALORIZATION}},
Year = {{2017}},

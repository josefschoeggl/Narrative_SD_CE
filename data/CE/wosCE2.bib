@article{ ISI:000439567500005,
Author = {Ghenta, Mihaela and Matei, Aniela},
Title = {{SMES AND THE CIRCULAR ECONOMY: FROM POLICY TO DIFFICULTIES ENCOUNTERED
   DURING IMPLEMENTATION}},
Journal = {{AMFITEATRU ECONOMIC}},

@article{ ISI:000437308600002,
Author = {Jaksic, Maja Levi and Rakicevic, Jovana and Jovanovic, Milica},
Title = {{SUSTAINABLE TECHNOLOGY AND BUSINESS INNOVATION FRAMEWORK-A COMPREHENSIVE
   APPROACH}},

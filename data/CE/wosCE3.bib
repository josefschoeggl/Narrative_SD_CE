@article{ ISI:000368206700045,
Author = {Supino, Stefania and Malandrino, Ornella and Testa, Mario and Sica,
   Daniela},
Title = {{Sustainability in the EU cement industry: the Italian and German
   experiences}},
Journal = {{JOURNAL OF CLEANER PRODUCTION}},

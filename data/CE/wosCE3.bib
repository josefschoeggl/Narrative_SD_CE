@article{ ISI:000387602800002,
Author = {Naims, Henriette},
Title = {{Economics of carbon dioxide capture and utilization-a supply and demand
   perspective}},
Journal = {{ENVIRONMENTAL SCIENCE AND POLLUTION RESEARCH}},
Year = {{2016}},
Volume = {{23}},
Number = {{22}},

@article{ ISI:000391079300083,
Author = {Ferreiro-Cabello, Javier and Fraile-Garcia, Esteban and de Pison
   Ascacibar, Eduardo Martinez and de Pison Ascacibar, Francisco Javier
   Martinez},
Title = {{Minimizing greenhouse gas emissions and costs for structures with flat
   slabs}},

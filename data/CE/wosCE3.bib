@article{ ISI:000365175000009,
Author = {Jacquet, Nicolas and Haubruge, Eric and Richel, Aurore},
Title = {{Production of biofuels and biomolecules in the framework of circular
   economy: A regional case study}},
Journal = {{WASTE MANAGEMENT \& RESEARCH}},

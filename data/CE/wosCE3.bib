@article{ ISI:000365802300035,
Author = {Gao, Lihui and Li, Shulei and Wang, Yongtian and Gui, Xiahui and Xu,
   Hongxiang},
Title = {{PRETREATMENT OF COKING WASTEWATER BY AN ADSORPTION PROCESS USING FINE
   COKING COAL}},
Journal = {{PHYSICOCHEMICAL PROBLEMS OF MINERAL PROCESSING}},
Year = {{2016}},
Volume = {{52}},
Number = {{1}},
Pages = {{422-436}},
Abstract = {{A new technique for pretreatment of coking wastewater is introduced
   based on the concept of circular economy. Coal is fed into a coking
   system after adsorption. This study validates the feasibility of using
   coking coal to adsorb organic pollutants in coking wastewater. The
   sorption kinetics and equilibrium sorption isotherms of coking coal for
   removal of chemical oxygen demand (COD) and phenol from coking
   wastewater was also discussed in this paper. Gas chromatograph/mass
   spectroscopy (GC/MS) was used to detect changes in the quality of coking
   wastewater. The results showed that when coking coal dosage was 120
   g/dm(3), 65\% of COD and 34\% of phenol in waste water can be removed
   after 40 min of agitation. The surface functional groups of coking coal
   before and after adsorption were observed with a Fourier transform
   infrared spectrometer. The kinetics of COD and phenol adsorption from

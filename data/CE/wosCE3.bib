@article{ ISI:000400824500017,
Author = {Sapsford, Devin and Cleall, Peter and Harbottle, Michael},
Title = {{In Situ Resource Recovery from Waste Repositories: Exploring the
   Potential for Mobilization and Capture of Metals from Anthropogenic Ores}},
Journal = {{JOURNAL OF SUSTAINABLE METALLURGY}},

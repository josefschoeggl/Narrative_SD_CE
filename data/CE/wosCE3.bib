@article{ ISI:000390621800039,
Author = {Medina, G. and Saez del Bosque, I. F. and Frias, M. and Sanchez de
   Rojas, M. I. and Medina, C.},
Title = {{Mineralogical study of granite waste in a pozzolan/Ca(OH)(2) system:
   Influence of the activation process}},
Journal = {{APPLIED CLAY SCIENCE}},
Year = {{2017}},
Volume = {{135}},
Pages = {{362-371}},
Month = {{JAN}},
Abstract = {{Despite the technological improvements introduced in the production of
   ornamental granite, sludge is inevitably generated in end product
   cutting and polishing. The present study constitutes the first attempt
   to scientifically analyse granite sludge, focusing in particular on its
   characterisation and pozzolanic activity and the morphological and
   microstructural changes taking place in both thermally activated waste
   and in the pozzolan/Ca(OH)(2) system in the first 360 days of reaction.
   The possible valorisation of this waste as a component of more
   eco-friendly cements is assessed with a view to furthering the circular
   economy. The findings show that thermal activation has no beneficial
   effect on sludge pozzolanicity, which at later ages was similar to the
   activity observed in other additions such as copper and manganese
   silicate slag but lower than in silica fume, fly ash and fired clay

@article{ ISI:000384626400001,
Author = {Geng, Yong and Fujita, Tsuyoshi and Park, Hung-suck and Chiu, Anthony S.
   F. and Huisingh, Donald},
Title = {{Recent progress on innovative eco-industrial development}},
Journal = {{JOURNAL OF CLEANER PRODUCTION}},
Year = {{2016}},
Volume = {{114}},
Pages = {{1-10}},
Month = {{FEB 15}},
Abstract = {{Due to rapid growth of urban populations and economic development, many
   countries and regions are facing severe challenges in seeking to respond
   to environmental pollution at both local and global levels. This Special
   Volume of the Journal of Cleaner Production reports upon recent progress
   on regenerative and preventative eco-industrial development (EID), based
   upon comprehensive reviews, innovative policies, metrics and indicators,
   tools and methods and the implementation of sustainable production and
   consumption. The forty-one articles in this Special Volume show that
   tangible benefits can be achieved from initiatives performed at both
   local and national levels. The results and recommendations provide
   critical insights on how to promote innovative eco-industrial
   development within different contexts. An integrated framework for
   promoting regenerative and preventative EID is proposed so that powerful
   synergies for addressing both local environmental quality and global
   impacts and ways to track these improvements through the governance
   framework, can be created. (C) 2015 Elsevier Ltd. All rights reserved.}},

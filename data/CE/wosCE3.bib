@article{ ISI:000387524000004,
Author = {Wu, Huaqing and Lv, Kui and Liang, Liang and Hu, Hanhui},
Title = {{Measuring performance of sustainable manufacturing with recyclable
   wastes: A case from China's iron and steel industry}},
Journal = {{OMEGA-INTERNATIONAL JOURNAL OF MANAGEMENT SCIENCE}},

Author = {Cerchier, P. and Pezzato, L. and Brunelli, K. and Dolcet, P. and
   Bartolozzi, A. and Bertani, R. and Dabala, M.},
Title = {{Antibacterial effect of PEO coating with silver on AA7075}},
Journal = {{MATERIALS SCIENCE \& ENGINEERING C-MATERIALS FOR BIOLOGICAL APPLICATIONS}},
Year = {{2017}},

@article{ ISI:000402090300004,
Author = {Malandrino, Ornella and Sica, Daniela and Testa, Mario and Supino,
   Stefania},
Title = {{Policies and Measures for Sustainable Management of Solar Panel
   End-of-Life in Italy}},
Journal = {{SUSTAINABILITY}},
Year = {{2017}},
Volume = {{9}},
Number = {{4}},
Month = {{APR}},
Abstract = {{The purpose of this work is to carry out a review of the main
   technical-economic and environmental implications associated with the
   production of photovoltaic (PV) energy, one of the renewable sources for
   the production of electricity which currently presents the highest rate
   of growth worldwide-particularly in Europe and in Italy. The review
   provides a detailed exploration of the most important initiatives taken
   at the national level for the end-of-life management of the modules, and
   highlights issues associated with the disposal and/or recycling of
   obsolete photovoltaic panels in terms of techno-economic and
   socio-environmental sustainability. The paper highlights the main
   critical elements and potential opportunities deriving from the
   technological, managerial, and organizational options available to
   enhance recovery and recycling rates of PV panels in Italy. Results
   point out the importance of a circular economy perspective, through the
   involvement and awareness of the actors in the process, in order to
   render an even greener photovoltaic energy life cycle.}},

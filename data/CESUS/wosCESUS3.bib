@article{ ISI:000418742400016,
Author = {Askoxylakis, Ioannis and Alexandris, George and Demetriou, Giorgos},
Title = {{Blockchain-enabled Intelligent Asset Exchange for a Circular Economy}},
Journal = {{ERCIM NEWS}},
Year = {{2017}},
Number = {{110, SI}},
Pages = {{27-28}},
Month = {{JUL}},
Abstract = {{As the notion of circular economy gains momentum, intelligent assets
   increasingly form the backbone of sustainable ecosystems. Although these
   assets can supply the necessary knowledge for fueling the value drivers
   of a circular economy, the generated value will be significantly
   amplified by allowing third parties to manage them and profit from
   better asset utilisation. However, for an everchanging networked
   environment consisting of numerous assets, ownership needs to be
   dynamic, granular and adaptable in order to maximise gains.
   Blockchain-based mechanisms can effectively serve this need by enabling
   transfer of asset ownership directly between parties participating in
   the circular economy while introducing trust, efficiency and automation
   in asset exchange contracts.}},

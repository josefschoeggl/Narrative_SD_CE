@article{ ISI:000412453500004,
Author = {Sun, Qiang},
Title = {{Research on the influencing factors of reverse logistics carbon
   footprint under sustainable development}},
Journal = {{ENVIRONMENTAL SCIENCE AND POLLUTION RESEARCH}},

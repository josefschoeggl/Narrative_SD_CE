@article{ ISI:000435587100097,
Author = {Velenturf, Anne P. M. and Purnell, Phil and Tregent, Mike and Ferguson,
   John and Holmes, Alan},
Title = {{Co-Producing a Vision and Approach for the Transition towards a Circular
   Economy: Perspectives from Government Partners}},

@article{ ISI:000390073600005,
Author = {Wang, Peng and Li, Wen and Kara, Sami},
Title = {{Cradle-to-cradle modeling of the future steel flow in China}},
Journal = {{RESOURCES CONSERVATION AND RECYCLING}},

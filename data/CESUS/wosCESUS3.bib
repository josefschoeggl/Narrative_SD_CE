@article{ ISI:000417046100002,
Author = {Dahiya, Shikha and Kumar, A. Naresh and Sravan, J. Shanthi and
   Chatterjee, Sulogna and Sarkar, Omprakash and Mohan, S. Venkata},
Title = {{Food waste biorefinery: Sustainable strategy for circular bioeconomy}},

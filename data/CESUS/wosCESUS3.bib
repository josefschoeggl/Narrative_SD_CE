@article{ ISI:000414879300135,
Author = {Saavedra, Yovana M. B. and Iritani, Diego R. and Pavan, Ana L. R. and
   Ometto, Aldo R.},
Title = {{Theoretical contribution of industrial ecology to circular economy}},

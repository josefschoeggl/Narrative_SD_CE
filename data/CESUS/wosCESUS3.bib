Author = {Kiran, Boda Ravi and Prasad, Majeti Narasimha Vara},
Title = {{Ricinus communis L. (Castor bean), a potential multi-purpose
   environmental crop for improved and integrated phytoremediation}},
Journal = {{EUROBIOTECH JOURNAL}},

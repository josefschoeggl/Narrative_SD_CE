@article{ ISI:000434902400031,
Author = {Whalen, Katherine A. and Berlin, Cecilia and Ekberg, Johanna and
   Barletta, Ilaria and Hammersberg, Peter},
Title = {{`All they do is win': Lessons learned from use of a serious game for
   Circular Economy education}},
Journal = {{RESOURCES CONSERVATION AND RECYCLING}},

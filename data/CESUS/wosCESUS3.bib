@article{ ISI:000419738000134,
Author = {Mestre, Ana and Cooper, Tim},
Title = {{Circular Product Design. A Multiple Loops Life Cycle Design Approach for
   the Circular Economy}},
Journal = {{DESIGN JOURNAL}},
Year = {{2017}},
Volume = {{20}},
Number = {{1}},
Pages = {{S1620-S1635}},
Note = {{12th International Conference of the European-Academy-of-Design (EAD),
   Sapienza Univ Rome, Fac Architecture, Rome, ITALY, APR 12-14, 2017}},
Organization = {{European Acad Design}},
Abstract = {{The circular economy is a high priority subject of discussion in the
   current political and academic contexts; however, practical approaches
   in relevant disciplines like design are in need of development. This
   article proposes a conceptual framework for circular product design,
   based on four multiple loops strategies: (I) design to slow the loops,
   (II) design to close the loops, (III) design for bio-inspired loops, and
   (IV) design for bio-based loops. Recent literature, notably on life
   cycle design strategies, the circular economy conceptual model and the
   European Commission's Circular Economy Package, is reviewed and product
   design cases illustrating each of the proposed are analysed. The article
   argues that different `circular' approaches centred upon the life cycle
   design phases can provide practical guiding strategies during the design
   process and thus promote sustainable design solutions for the circular
   economy within the United Nation's sustainable development goals.}},

@article{ ISI:000435188000191,
Author = {Mew, Michael C. and Steiner, Gerald and Geissler, Bernhard},
Title = {{Phosphorus Supply Chain-Scientific, Technical, and Economic Foundations:
   A Transdisciplinary Orientation}},

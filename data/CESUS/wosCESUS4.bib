@article{ ISI:000404951800002,
Author = {Villena Manzanares, Francisco and Galiano Coronil, Araceli},
Title = {{SUSTAINABLE URBAN DEVELOPMENT AND ITS IMPLICATIONS FOR ENTERPRISES AND
   TERRITORIES}},
Journal = {{REVISTA DE ESTUDIOS EMPRESARIALES-SEGUNDA EPOCA}},
Year = {{2017}},
Number = {{1}},
Pages = {{5-15}},
Abstract = {{The present article aims to show the role of sustainable development,
   emphasizing the important role that it plays in companies and the
   territory. In a new vision of circular economy, sustainable development
   becomes a source capable of generating eco-innovation and productive
   models based on the new ``green{''} economy. In short, it aims to give a
   holistic view of sustainable development and reflect on their impact on

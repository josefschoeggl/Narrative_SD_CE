@article{ ISI:000356450800012,
Author = {Zhu, Qinghua and Geng, Yong and Sarkis, Joseph and Lai, Kee-Hung},
Title = {{Barriers to Promoting Eco-Industrial Parks Development in China:
   Perspectives from Senior Officials at National Industrial Parks}},
Journal = {{JOURNAL OF INDUSTRIAL ECOLOGY}},

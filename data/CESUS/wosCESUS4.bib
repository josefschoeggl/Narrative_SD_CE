@article{ ISI:000381545200003,
Author = {Yu, Xiaoman and Geng, Yong and Dong, Huijuan and Fujita, Tsuyoshi and
   Liu, Zhe},
Title = {{Emergy-based sustainability assessment on natural resource utilization
   in 30 Chinese provinces}},

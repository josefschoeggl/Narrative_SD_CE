@article{ ISI:000309375100033,
Author = {Liu, Dehai and Li, Hongyi and Wang, Weiguo and Dong, Yucheng},
Title = {{Constructivism scenario evolutionary analysis of zero emission regional
   planning: A case of Qaidam Circular Economy Pilot Area in China}},
Journal = {{INTERNATIONAL JOURNAL OF PRODUCTION ECONOMICS}},

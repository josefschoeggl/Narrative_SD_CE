@article{ ISI:000217252200008,
Author = {Zhong Taryang and Huang Xianjin and Li Lulu and Wang Chen},
Title = {{Comprehensive Assessment of Regional Circular Economy Development Based
   on Efficiency of Resource \& Environment: A Case Study of Jiangsu
   Province}},

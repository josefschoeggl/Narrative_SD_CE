@article{ ISI:000391558100015,
Author = {Sun, Xixiang and Li, Juan and Li, Li},
Title = {{Estimating circular agricultural efficiency using dea methods}},
Journal = {{AGRO FOOD INDUSTRY HI-TECH}},
Year = {{2016}},
Volume = {{27}},
Number = {{6}},
Pages = {{94-98}},
Month = {{NOV-DEC}},
Abstract = {{The excessive investments in agricultural land and wastes in
   agricultural production pose great threats to ecological sustainability.
   This study evaluates the efficiency of circular agriculture in
   addressing these problems. An input and output evaluation index system
   is constructed to assess the efficiency of circular agriculture. The
   input evaluation indexes include agricultural employees, chemical
   fertilizer consumption, energy consumption of agriculture, and sown area
   of crops, while the output evaluation indexes include total agricultural
   output, rate of cultivated area output, and output of major agricultural
   products. An input-oriented data envelopment analysis model is adopted
   to estimate the circular agriculture efficiency of the 17 regions of
   Hubei Province in China. Ten of these regions have been identified as
   fully efficient, while the other seven regions are regarded as lowly
   efficient. The low efficiency scores of the seven regions are analyzed,
   and several countermeasures for promoting circular agricultural
   efficiency are provided. This work contributes novel ideas for
   developing circular agriculture and offers a solid basis for
   ecologicalization and circular economy.}},
